CREATE TABLE Towar (ID number(10) NOT NULL, Nazwa varchar2(255), Kod varchar2(255), Ilostan double precision NOT NULL, "Czas zalegania" number(10) NOT NULL, "Ilosc zablokowana" double precision NOT NULL, Zalega number(1) NOT NULL, "Potrzebuje zamówienia" number(1) NOT NULL, "Ilośc minimalna" double precision NOT NULL, PRIMARY KEY (ID));
CREATE TABLE Lokalizacja (ID number(10) NOT NULL, RegałID number(10) NOT NULL, Kod varchar2(255), "Numer rzedu" number(10) NOT NULL, "Numer sektora" number(10) NOT NULL, Zajeta number(1) NOT NULL, PRIMARY KEY (ID));
