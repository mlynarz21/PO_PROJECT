CREATE TABLE Zamówienie (ID number(10) NOT NULL, IDZamowienia number(10), Kod varchar2(255), DataZlożenia date, Status number(10), "Typ odbioru" number(10), Status2 number(10), TerminRealizacji date, Discriminator varchar2(255) NOT NULL, PRIMARY KEY (ID));
