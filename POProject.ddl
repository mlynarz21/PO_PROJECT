CREATE TABLE Adres (ID number(10) NOT NULL, "Kod pocztowy" varchar2(255), Ulica varchar2(255), Miasto varchar2(255), Numer number(10) NOT NULL, KlientID number(10) NOT NULL, PRIMARY KEY (ID));
